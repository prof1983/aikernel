#ifndef AiKernel_H
#define AiKernel_H

#include "A.h"
#include "ABase"
#include "AOwl"

typedef AInt AEngine;
typedef AInt AEnviroment;
typedef AInt ARunner;

/** Initializes agenda
 *  @return AError value
 */
AError
func Agenda_Init();

/** Initializes AiKernel
 *  @return AError value
 */
AError
func AiKernel_Init();

/** Return engine enviroment
 *  @return Engine enviroment
 */
AEnviroment
func AiKernel_GetEngineEnviroment(AEngine Engine);

/** Return engine runner
 *  @return Engine runner
 */
ARunner
func AiKernel_GetEngineRunner(AEngine Engine);

#endif // AiKernel_H
